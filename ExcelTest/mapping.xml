<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="2" />
		<loop startRow="3" endRow="3" items="addresses" var="address" varType="com.namoo.excel.Address">
			<section startRow="3" endRow="3">
				<mapping row="3" col="0">address.name</mapping>
				<mapping row="3" col="1">address.phone</mapping>
				<mapping row="3" col="2">address.address</mapping>
			</section> 
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>